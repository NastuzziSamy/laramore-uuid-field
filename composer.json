{
    "name": "laramore/uuid-field",
    "description": "Add an UUID field for Laramore",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Laramore\\": "src/"
        }
    },
    "require": {
        "ramsey/uuid": "^3.8",
        "laramore/laramore": ">=0.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "NastuzziSamy",
            "email": "samy@nastuzzi.fr"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Laramore\\Providers\\UuidProvider"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.11.7",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6"
    },
    "suggest": {
        "laramore/migrations": "Generate automatically all your migrations"
    }
}
